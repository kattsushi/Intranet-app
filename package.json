    "bcrypt": "^0.8.5",
    "bcryptjs": "^2.2.1",
    "body-parser": "^1.13.3",
    "colors": "^1.1.2",
    "cookie-parser": "^1.3.5",
    "crypto": "0.0.3",
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "express-hbs": "^0.8.4",
    "express-session": "^1.11.3",
    "express-validator": "^2.16.0",
    "gravatar": "^1.3.0",
    "lodash": "^3.10.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.10.6",
    "morgan": "^1.6.1",
    "multer": "^1.0.3",
