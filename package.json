{
  "name": "Intranet",
  "description": "Aplicacion nodejs + express + Posgresql + handlerbars + sass + gulp",
  "version": "0.2.0",
  "private": false,
  "scripts": {
    "start": "node server/bin/www"
  },
  "homepage": "https://github.com/kattsushi/Intranet-app/",
  "licenses": [
    {
      "type": "MIT",
