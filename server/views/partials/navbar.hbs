<md-toolbar>
    <div class="rigth md-toolbar-tools" >
        <md-button class="md-icon-button"
            aria-label="Menu Button"

            ng-click="vm.onClickMenu();">
            <md-icon
                md-svg-icon="svg/menu.svg"
                aria-label="Menu Icon">
            </md-icon>
        </md-button>
        <img src="img/logoblanco.gif" alt="logo" style="max-height:90px;padding-right:10px" />
        <span hide show-gt-sm flex>Intranet</span> </h2>
          <span flex></span>
        <md-button aria-label="Inicio" href="/#!" hide show-gt-sm flex  >
          Inicio
        </md-button>
        <md-button aria-label="Agenda" hide show-gt-sm flex>
          Agenda
        </md-button>
        <md-button aria-label="Empleado" hide show-gt-sm flex
            ng-show="usrConectado.estaConectado">
          Empleado
        </md-button>
        <md-button class="md-raised"
            aria-label="Administración" hide show-gt-sm flex
            ng-hide="usrConectado.nivel <= 50 ">
          Administración
        </md-button>
        <md-button href="/#!/inicio"
          class="md-raised md-primary"
          aria-label="Iniciar Sesion"
          ng-hide="usrConectado.estaConectado"  >
          Iniciar Sesion
        </md-button>
        <md-button class="md-raised md-warn"
          aria-label="Salir"
          ng-show="usrConectado.estaConectado"
          ng-click="vm.salir()" >
          Salir
        </md-button>
    </div>
</md-toolbar>
